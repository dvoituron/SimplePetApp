  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run SimplePetApp (dotnet run)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "run-MyPetVenues",
      "program": "${workspaceFolder}/MyPetVenues/bin/Debug/net9.0/MyPetVenues.dll",
      "cwd": "${workspaceFolder}/MyPetVenues",
      "stopAtEntry": false,
      "justMyCode": true,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      }
    }
  ]
}